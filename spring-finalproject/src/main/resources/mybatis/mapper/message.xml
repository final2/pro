<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.finalproject.dao.MessageDao">
	<select id="getReceiveMessages" resultType="Message">
		select
			no, 
			from_name	as	"from", 
			to_name		as	"to", 
			contents, 
			receiving, 
			regdate
		from
			messenger
	</select>
</mapper>




